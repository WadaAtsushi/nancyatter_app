<div class="container w-75 mt-5">
  <%= tag.h1 "appメンバー一覧" %>
    <table class="table">
      <thead>
        <%= tag.tr %>
          <%= tag.th "image" %>
          <%= tag.th "nickname" %>
          <%= tag.th "user" %>
          <%= tag.th "app" %>
      </thead>
      　
      <tbody>
        <% @menburs.each do |menbur| %>
          <% if menbur.del_flag!=1 %>
            <%= tag.tr %>
              <%= tag.td image_tag menbur.profile_image.url, class: "profile-img", style:"width:80px;border-radius:30%;" %>
              <%= tag.td menbur.nickname %>
              <%= tag.td menbur.user.name %>
              <%= tag.td menbur.nancyatterapp.app_name %>
          <% end %>
        <% end %>    
      </tbody>
    </table>

</div>
